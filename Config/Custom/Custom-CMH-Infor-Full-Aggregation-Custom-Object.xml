<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Custom PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Custom name="CMH-Infor-Aggregation-Custom-Object">
  <Attributes>
    <Map>
      <entry key="applicationName" value="%%CMH-Infor-ApplicationName%%"/>
      <entry key="lastRunTimeFullTimeStamp"/>
      <entry key="lastRunIncrementalTimeStamp"/>
      <entry key="sqlQueryFull" value="SELECT * FROM users WHERE PrimaryWorkAssignment='YES';"/>
      <entry key="sqlQueryIncremental" value="SELECT * FROM users WHERE PrimaryWorkAssignment='YES' and DateEmployeeRecordCreated&gt;='TIME_STAMP' or DateEmployeeRecordUpdated&gt;='TIME_STAMP'"/>
      <entry key="taskName" value="Infor HR Database Aggregation"/>
    </Map>
  </Attributes>
</Custom>
