<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Custom PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Custom name="CMH-DateFormat">
  <Attributes>
    <Map>     
      <entry key="preIterationTimestamp" value="yyyy-MM-dd hh:mm:ss"/>
      <entry key="renameUserTimestamp" value="ddMMMyyyy"/>
      <entry key="userNameFileNameTimestamp" value="yyyyMMdd"/>
      <entry key="userNameGenerationTimestamp" value="yyyy-MM-dd hh:mm:ss a"/>
	  <entry key="genericDateFormat" value="MM-dd-yyyy"/>
	  <entry key="legalHoldTimestamp" value="yyyy-MM-dd_hh-mm-ss.a"/>
	  <entry key="genericViewDateFormat" value="yyyy-MM-dd"/>
    </Map>
  </Attributes>
</Custom>