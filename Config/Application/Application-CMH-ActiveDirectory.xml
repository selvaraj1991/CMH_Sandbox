        <Field displayName="con_prov_policy_ad_description" helpKey="help_con_prov_policy_ad_description" name="description" section="General" type="string">
          <Script>
            <Source><![CDATA[
            
            import sailpoint.object.Identity;
            
            String userType = identity.getStringAttribute("userType");
			String description = identity.getStringAttribute("employeeId");
			String displayName = "";
			if(userType.equalsIgnoreCase("NONEMPLOYEE")){
				Identity mangerIden = identity.getManager();
				if(mangerIden!=null){
					String initials = mangerIden.getStringAttribute("middleInitial");
					String lastName = mangerIden.getLastname();
				    if(lastName!=null){
				    	displayName = displayName.concat(lastName);
				    }
				    String firstName = mangerIden.getFirstname();
				    if(firstName!=null){
				    	if(displayName!=null && !displayName.isEmpty()){
					    	displayName = displayName.concat(", ");
					    }
				    	displayName = displayName.concat(firstName);
				    }
				    if(initials!=null && !initials.isEmpty()){
				    	displayName = displayName.concat(" ").concat(initials).concat(".");
					}
				}
			}
			
			if(description!=null && displayName!=null && !displayName.isEmpty()){
				description = description +" "+displayName;
			}
			return description;
			]]></Source>
          </Script>
        </Field>
