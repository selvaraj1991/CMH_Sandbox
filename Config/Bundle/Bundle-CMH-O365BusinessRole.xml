<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Bundle PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Bundle displayName="O365 Business Role" name="O365 Business Role" type="business">
  <Attributes>
    <Map>
      <entry key="accountSelectorRules"/>
      <entry key="allowDuplicateAccounts" value="false"/>
      <entry key="allowMultipleAssignments" value="false"/>
      <entry key="mergeTemplates" value="false"/>
      <entry key="sysDescriptions">
        <value>
          <Map>
            <entry key="en_US"/>
          </Map>
        </value>
      </entry>
    </Map>
  </Attributes>
  <Inheritance>
    <Reference class="sailpoint.object.Bundle" name="CMH User Roles"/>
  </Inheritance>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <Requirements>
    <Reference class="sailpoint.object.Bundle" name="O365 IT Role"/>
  </Requirements>
  <Selector>
    <IdentitySelector>
      <RuleRef>
        <Reference class="sailpoint.object.Rule" name="CMH-O365-Role-Assignment-Rule"/>
      </RuleRef>
    </IdentitySelector>
  </Selector>
</Bundle>
