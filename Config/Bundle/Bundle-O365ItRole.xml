<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Bundle PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Bundle displayName="O365 IT Role" name="O365 IT Role" type="it">
  <Attributes>
    <Map>
      <entry key="accountSelectorRules"/>
      <entry key="allowDuplicateAccounts" value="false"/>
      <entry key="allowMultipleAssignments" value="false"/>
      <entry key="mergeTemplates" value="false"/>
      <entry key="sysDescriptions">
        <value>
          <Map>
            <entry key="en_US"/>
          </Map>
        </value>
      </entry>
    </Map>
  </Attributes>
  <Inheritance>
    <Reference class="sailpoint.object.Bundle" name="O365 Business Role"/>
  </Inheritance>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <Profiles>
    <Profile>
      <ApplicationRef>
        <Reference class="sailpoint.object.Application" name="%%CMH-AD-ApplicationName%%"/>
      </ApplicationRef>
      <Constraints>
        <Filter operation="CONTAINS_ALL" property="memberOf">
          <Value>
            <List>
              <String>CN=CMH_Email_Enabled,OU=Groups,OU=CMHUsers,DC=cmhAd,DC=com</String>
              <String>CN=O365-E5Lic,OU=Groups,OU=CMHUsers,DC=cmhAd,DC=com</String>
              <String>CN=O365-EMSLic,OU=Groups,OU=CMHUsers,DC=cmhAd,DC=com</String>
            </List>
          </Value>
        </Filter>
      </Constraints>
    </Profile>
  </Profiles>
</Bundle>
